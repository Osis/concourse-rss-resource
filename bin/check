#!/usr/bin/env ruby
# frozen_string_literal: true

require 'json'

input = JSON.parse(STDIN.read)

warn 'Checking source:'
warn input['source']
warn "for version #{input['version']}"

output = [
  { 'ref' => '61cebf' },
]

if input['version']
  output << { 'ref' => 'd74e01' }
  output << { 'ref' => '7154fe' }
end

puts JSON.generate(output)
